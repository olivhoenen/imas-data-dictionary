	<xs:complexType name="oblique_static">
		<xs:annotation>
			<xs:documentation>Description of a 2D parallelogram</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r">
				<xs:annotation>
					<xs:documentation>Major radius of the reference point (from which the alpha and beta angles are defined, marked by a + on the diagram)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="z">
				<xs:annotation>
					<xs:documentation>Height of the reference point (from which the alpha and beta angles are defined, marked by a + on the diagram)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="length_alpha">
				<xs:annotation>
					<xs:documentation>Length of the parallelogram side inclined with angle alpha with respect to the major radius axis</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="length_beta">
				<xs:annotation>
					<xs:documentation>Length of the parallelogram side inclined with angle beta with respect to the height axis</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="alpha">
				<xs:annotation>
					<xs:documentation>Inclination of first angle measured counter-clockwise from horizontal outwardly directed radial vector (grad R).</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>rad</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="beta">
				<xs:annotation>
					<xs:documentation>Inclination of second angle measured counter-clockwise from vertically upwards directed vector (grad Z). If both alpha and beta are zero (rectangle) then the simpler rectangular elements description should be used.</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>rad</units>
						<url>utilities/parallelogram.svg</url>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="arcs_of_circle_static">
