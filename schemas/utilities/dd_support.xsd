					<xs:appinfo>
						<type>static</type>
						<units>m</units>
						<coordinate1>../../types</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
