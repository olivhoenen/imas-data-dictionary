					<xs:appinfo>
						<coordinate1>../grid/rho_tor_norm</coordinate1>
						<type>dynamic</type>
						<units>-</units>
						<cocos_label_transformation>q_like</cocos_label_transformation>
						<cocos_transformation_expression>.fact_q</cocos_transformation_expression>
						<cocos_leaf_name_aos_indices>IDSPATH.q</cocos_leaf_name_aos_indices>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="magnetic_shear">
				<xs:annotation>
					<xs:documentation>Magnetic shear, defined as rho_tor/q . dq/drho_tor</xs:documentation>
					<xs:appinfo>
						<coordinate1>../grid/rho_tor_norm</coordinate1>
						<units>-</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="time" type="flt_type">
				<xs:annotation>
					<xs:documentation>Time</xs:documentation>
					<xs:appinfo>
						<units>s</units>
						<type>dynamic</type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
