	<xs:complexType name="fibre_bundle">
		<xs:annotation>
			<xs:documentation>Fibre bundle</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="geometry" type="detector_aperture">
				<xs:annotation>
					<xs:documentation>Geometry of the fibre bundle entrance</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fibre_radius">
				<xs:annotation>
					<xs:documentation>Radius of a single fibre</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="fibre_positions" type="x1x21d_static">
				<xs:annotation>
					<xs:documentation>Individual fibres centres positions in the (X1, X2) coordinate system</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="polarizer">
		<xs:annotation>
			<xs:documentation>Generic description of a polarizer (extension of the detector_aperture complex type)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="centre" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>If geometry_type=2, coordinates of the centre of the circle. If geometry_type=1 or 3, coordinates of the origin of the local coordinate system (X1,X2,X3) describing the plane polarizer. This origin is located within the polarizer area. Note that there is some flexibility here and the data provider should choose the most convenient coordinate system for the object, respecting the definitions of (X1,X2,X3) indicated below.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="radius">
				<xs:annotation>
					<xs:documentation>Radius of the circle, used only if geometry_type = 2</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="x1_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X1 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X1 vector is more horizontal than X2 (has a smaller abs(Z) component) and oriented in the positive phi direction (counter-clockwise when viewing from above). </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="x2_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X2 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X2 axis is orthonormal so that uX2 = uX3 x uX1.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="x3_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X3 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X3 axis is normal to the polarizer plane and oriented towards the plasma.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="polarization_angle">
				<xs:annotation>
					<xs:documentation>Alignment angle of the polarizer in the (x1,x2) plane. Electric fields parallel to the polarizer angle will be reflected. The angle is defined with respect to the x1 unit vector, positive in the counter-clockwise direction when looking towards the plasma</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>rad</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_2points_rz">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points, in R and Z only</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the second point</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_2points">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the second point</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_2points_rphiz_2d">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rphiz2d_static">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rphiz2d_static">
				<xs:annotation>
					<xs:documentation>Position of the second point</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_2points_dynamic_aos1">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points, dynamic within an AoS1 (1st point fixed, 2nd point is dynamic)</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rphiz1d_dynamic_aos1_common_time">
				<xs:annotation>
					<xs:documentation>Position of the second point (possibly dynamic)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="moving_mode" type="identifier">
				<xs:annotation>
					<xs:documentation>Moving mode of the line of sight. Index = 0 : no movement, fixed position. Index = 1 : oscillating</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="position_parameter" type="signal_flt_1d">
				<xs:annotation>
					<xs:documentation>In case of line of sight moving during a pulse, position parameter allowing to record and compute the line of sight position as a function of time</xs:documentation>
					<xs:appinfo>
						<units>mixed</units>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="amplitude_parameter">
				<xs:annotation>
					<xs:documentation>Amplitude of the line of sight position parameter oscillation (in case moving_mode/index = 1)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>mixed</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="period">
				<xs:annotation>
					<xs:documentation>Period of the line of sight oscillation (in case moving_mode/index = 1)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>s</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_2points_dynamic_aos3">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points, dynamic within a type 3 array of structures (index on time)</xs:documentation>
			<xs:appinfo>
				<aos3Parent>yes</aos3Parent>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rphiz0d_dynamic_aos3">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rphiz0d_dynamic_aos3">
				<xs:annotation>
					<xs:documentation>Position of the second point</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="line_of_sight_3points">
		<xs:annotation>
			<xs:documentation>Generic description of a line of sight, defined by two points (one way) and an optional third point to indicate the direction of reflection if the second point is e.g. the position of a mirror reflecting the line-of-sight</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="first_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the first point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="second_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the second point</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="third_point" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the third point</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="camera_geometry">
		<xs:annotation>
			<xs:documentation>Camera geometry. The orientation of the camera is described as follows : pixels are aligned along x1 and x2 unit vectors while x3 is normal to the detector plane.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="pixel_dimensions">
				<xs:annotation>
					<xs:documentation>Pixel dimension in each direction (x1, x2)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...2</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="pixels_n">
				<xs:annotation>
					<xs:documentation>Number of pixels in each direction (x1, x2)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...2</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="INT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="pixel_position" type="rphiz2d_static">
				<xs:annotation>
					<xs:documentation>Position of the centre of each pixel. First dimension : line index (x1 axis). Second dimension: column index (x2 axis).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="camera_dimensions">
				<xs:annotation>
					<xs:documentation>Total camera dimension in each direction (x1, x2)</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<coordinate1>1...2</coordinate1>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="centre" type="rphiz0d_static">
				<xs:annotation>
					<xs:documentation>Position of the camera centre</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="x1_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X1 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X1 vector is more horizontal than X2 (has a smaller abs(Z) component) and oriented in the positive phi direction (counter-clockwise when viewing from above). </xs:documentation>
					<xs:appinfo>
						<url>utilities/detector_aperture_coordinates.svg</url>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="x2_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X2 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X2 axis is orthonormal so that uX2 = uX3 x uX1.</xs:documentation>
					<xs:appinfo>
						<url>utilities/detector_aperture_coordinates.svg</url>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="x3_unit_vector" type="xyz0d_static">
				<xs:annotation>
					<xs:documentation>Components of the X3 direction unit vector in the (X,Y,Z) coordinate system, where X is the major radius axis for phi = 0, Y is the major radius axis for phi = pi/2, and Z is the height axis. The X3 axis is normal to the camera plane and oriented towards the plasma.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="line_of_sight" type="line_of_sight_2points_rphiz_2d">
				<xs:annotation>
					<xs:documentation>Description of the line of sight for each pixel, given by 2 points. For each coordinate : first dimension : line index (x1 axis); second dimension: column index (x2 axis).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="optical_element_material">
		<xs:annotation>
			<xs:documentation>Material of an optical element of a camera</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="identifier_static">
				<xs:annotation>
					<xs:documentation>Type of optical element material. In case of 'metal' refractive_index and extinction_coefficient are used. In case of 'dielectric' refractive_index and transmission_coefficient are used.</xs:documentation>
					<xs:appinfo>
						<doc_identifier>utilities/optical_element_material_identifier.xml</doc_identifier>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="wavelengths">
				<xs:annotation>
					<xs:documentation>Wavelengths array for refractive_index, extinction_coefficient and transmission_coefficient</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>m</units>
						<coordinate1>1...N</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="refractive_index">
				<xs:annotation>
					<xs:documentation>Refractive index (for metal and dielectric)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../wavelengths</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="extinction_coefficient">
				<xs:annotation>
					<xs:documentation>Extinction coefficient (for metal)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../wavelengths</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="transmission_coefficient">
				<xs:annotation>
					<xs:documentation>Transmission coefficient (for dielectric)</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../wavelengths</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="roughness">
				<xs:annotation>
					<xs:documentation>Roughness parameter of the material. Varies in range [0, 1]. 0 is perfectly specular, 1 is perfectly rough</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
						<units>-</units>
						<coordinate1>../wavelengths</coordinate1>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_1D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="optical_element">
		<xs:annotation>
			<xs:documentation>Optical element of a camera</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type" type="identifier_static">
				<xs:annotation>
					<xs:documentation>Type of optical element. In case of 'mirror' and 'diaphragm', the element is described by one 'front_surface'. In case of 'lens', the element is described by 'front_surface' and 'back_surface'.</xs:documentation>
					<xs:appinfo>
						<doc_identifier>utilities/optical_element_identifier.xml</doc_identifier>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="front_surface" type="curved_surface">
				<xs:annotation>
					<xs:documentation>Curvature of the front surface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="back_surface" type="curved_surface">
				<xs:annotation>
					<xs:documentation>Curvature of the front surface</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="thickness">
				<xs:annotation>
					<xs:documentation>Distance between front_surface and back_surface along the X3 vector</xs:documentation>
					<xs:appinfo>
