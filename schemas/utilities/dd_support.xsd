			<xs:documentation>Version of the access layer package used to PUT this IDS</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="data_dictionary">
				<xs:annotation>
					<xs:documentation>Version of Data Dictionary used to PUT this IDS</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="STR_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="access_layer">
				<xs:annotation>
					<xs:documentation>Version of Access Layer used to PUT this IDS</xs:documentation>
					<xs:appinfo>
						<type>constant</type>
