	<xs:complexType name="coil_cross_section">
		<xs:annotation>
			<xs:documentation>Coil cross-section</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="geometry_type" type="identifier_static">
				<xs:annotation>
					<xs:documentation>Geometry type used to describe the cross section of this element. The conductor centre is given by the ../../elements description.</xs:documentation>
					<xs:appinfo>
						<doc_identifier>utilities/surface_geometry_identifier.xml</doc_identifier>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="width">
				<xs:annotation>
					<xs:documentation>Full width of the rectangle or square in the normal direction, when geometry_type/index = 3 or 4. Diameter of the circle when geometry_type/index = 2. Outer diameter of the annulus in case geometry_type/index = 5</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="height">
				<xs:annotation>
					<xs:documentation>Full height of the rectangle in the binormal direction, used only if geometry_type/index = 3</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="radius_inner">
				<xs:annotation>
					<xs:documentation>Inner radius of the annulus, used only if geometry_type/index = 5</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="outline" type="normal_binormal_static">
				<xs:annotation>
					<xs:documentation>Polygonal outline of the cross section in the (normal, binormal) coordinate system. Do NOT repeat the first point.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="area">
				<xs:annotation>
					<xs:documentation>Area of the conductor cross-section, derived from the above geometric data</xs:documentation>
					<xs:appinfo>
						<type>static</type>
						<units>m^2</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="coil_na_rphiz1d_static">
		<xs:annotation>
			<xs:documentation>Structure for list of R, Z, Phi positions (1D, static), with a reference to the types coordinate specific to this IDS</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="r">
