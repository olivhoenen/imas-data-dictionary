				</xs:annotation>
				<xs:complexType>
					<xs:group ref="FLT_0D"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="width">
				<xs:annotation>
					<xs:documentation>Horizontal full width</xs:documentation>
