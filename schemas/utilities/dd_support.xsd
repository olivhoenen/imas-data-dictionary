				</xs:complexType>
			</xs:element>
			<xs:element name="rho_pol_norm">
				<xs:annotation>
					<xs:documentation>Normalized poloidal flux coordinate = sqrt((psi(rho)-psi(magnetic_axis)) / (psi(LCFS)-psi(magnetic_axis)))</xs:documentation>
					<xs:appinfo>
						<type>dynamic</type>
						<coordinate1>../rho_tor_norm</coordinate1>
						<units>-</units>
					</xs:appinfo>
				</xs:annotation>
				<xs:complexType>
